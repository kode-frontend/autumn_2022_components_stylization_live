{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "run-expo": "expo start",
    "run-android": "expo start --android",
    "run-ios": "expo start --ios",
    "run-web": "expo start --web",
    "start": "npm-run-all prestart run-expo",
    "android": "npm-run-all prestart run-android",
    "ios": "npm-run-all prestart run-ios",
    "web": "npm-run-all prestart run-web",
    "update-icons": "ts-node-transpile-only --project node.tsconfig.json scripts/transform-svg/transform-svg.ts",
    "prestart": "sb-rn-get-stories",
    "storybook-watcher": "sb-rn-watcher"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "~1.15.0",
    "@react-native-community/datetimepicker": "3.5.2",
    "@react-native-community/slider": "4.1.12",
    "@react-navigation/native": "^6.0.4",
    "@react-navigation/native-stack": "^6.2.2",
    "@storybook/addon-controls": "6.3",
    "@storybook/addon-ondevice-backgrounds": "^6.0.1-beta.9",
    "@storybook/addon-ondevice-controls": "^6.0.1-beta.9",
    "@storybook/addon-ondevice-notes": "^6.0.1-beta.9",
    "expo": "^43.0.0",
    "expo-app-loading": "~1.2.1",
    "expo-font": "~10.0.3",
    "expo-status-bar": "~1.1.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-native": "0.64.2",
    "react-native-safe-area-context": "3.3.2",
    "react-native-screens": "~3.8.0",
    "react-native-svg": "12.1.1",
    "react-native-web": "0.17.1",
    "styled-components": "^5.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@storybook/addon-actions": "6.3",
    "@storybook/addon-ondevice-actions": "^6.0.1-beta.9",
    "@storybook/react-native": "^6.0.1-beta.9",
    "@storybook/react-native-server": "^5.3.23",
    "@svgr/core": "^5.5.0",
    "@types/prettier": "^2.4.1",
    "@types/react": "~17.0.21",
    "@types/react-native": "~0.64.12",
    "@types/styled-components": "^5.1.15",
    "@types/styled-components-react-native": "^5.1.1",
    "babel-loader": "^8.2.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "react-native-storybook-loader": "^2.0.4",
    "ts-node": "^10.4.0",
    "typescript": "~4.3.5"
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "./src"
      ],
      "pattern": "**/*.story.tsx",
      "outputFile": "./storybook/storyLoader.js"
    }
  },
  "private": true
}
